{
    "name": "laravel-merax/avatars",
    "type": "library",
    "license": "MIT",
    "description": "User Avatar manager dependency",
    "authors": [
        {
            "name": "Sergiu Popa",
            "email": "popa.sergiu920@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://gitlab.dev-task.ru/laravel-merax/file-server.git"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "laravel-enso/image-transformer": "^1.6|^2.0",
        "laravel-merax/file-server": "dev-master|dev-laravel-8",
        "laravolt/avatar": "^3.0|^4.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelMerax\\Avatars\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelMerax\\Avatars\\AppServiceProvider"
            ],
            "aliases": {}
        }
    }
}
